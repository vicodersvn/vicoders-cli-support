{
  "name": "@vicoders/cli-suppoprt",
  "version": "1.0.0",
  "description": "Vicoders CLI Support Module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json && rm -rf ../node_modules/@vicoders/cli-support/dist && cp -R dist ../node_modules/@vicoders/cli-support",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "publish_module_only": "npm run build && npm publish --access public"
  },
  "keywords": [
    "Vicoders",
    "CLI",
    "Support"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Hieupv <hieupv@codersvn.com>",
  "dependencies": {
    "@angular-devkit/core": "^9.0.2",
    "@angular-devkit/schematics": "^9.0.2",
    "@schematics/angular": "^9.0.2",
    "@types/colors": "^1.2.1",
    "@types/decompress": "^4.2.3",
    "@types/inquirer": "^6.5.0",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "@types/pluralize": "^0.0.29",
    "@types/shelljs": "^0.8.6",
    "@vicoders/console": "^2.0.0",
    "colors": "^1.4.0",
    "decompress": "^4.2.0",
    "inquirer": "^7.0.4",
    "jasmine": "^3.3.1",
    "ora": "^4.0.3",
    "pluralize": "^8.0.0",
    "shelljs": "^0.8.3",
    "typescript": "~3.5.3"
